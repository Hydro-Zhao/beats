{
  "description": "Pipeline for parsing gem stats logs",
  "processors" : [
      {
          "convert": {
              "field": "gem5.stats.system.cpu.numCycles",
              "type": "long"
          }
      },
      {
          "convert": {
              "field": "gem5.stats.system.cpu.iew.iewExecutedInsts",
              "type": "long"
          }
      },
      {
          "convert": {
              "field": "gem5.stats.system.l2.overall_access::total",
              "type": "long"
          }
      },
      {
          "convert": {
              "field": "gem5.stats.system.cpu.icache.overall_access::total",
              "type": "long"
          }
      },
      {
          "convert": {
              "field": "gem5.stats.system.mem_ctrls.num_writes::total",
              "type": "long"
          }
      },
      {
          "convert": {
              "field": "gem5.stats.system.mem_ctrls.num_reads::total",
              "type": "long"
          }
      }
  ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
