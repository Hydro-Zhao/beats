{
  "description": "Pipeline for parsing gem dram logs",
  "processors": [
      {
          "grok": {
              "field": "message",
              "patterns": ["^%{SPACE}%{NUMBER}: system.mem_ctrls: Memory capacity %{NUMBER:gem5.dram.memory_capacity_bytes:integer}", 
                  "^%{SPACE}%{NUMBER}: system.mem_ctrls: Row buffer size %{NUMBER:gem5.dram.row_buffer_size_bytes:integer} bytes with %{NUMBER:gem5.dram.columns_per_row_buffer:integer}",
                  "^%{SPACE}%{NUMBER:gem5.dram.tick:long}: system.mem_ctrls: %{TYPE}: %{READ_REQ:gem5.dram.access_type:keyword} %{HEX_ADRESS:gem5.dram.address:keyword}",
                  "^%{SPACE}%{NUMBER:gem5.dram.tick:long}: system.mem_ctrls: %{TYPE}: %{WRITE_REQ:gem5.dram.access_type:keyword} %{HEX_ADRESS:gem5.dram.address:keyword}"], 
              "pattern_definitions": {
                  "READ_REQ": "ReadReq",
                  "WRITE_REQ": "WriteReq",
                  "HEX_ADRESS": "0[xX][0-9a-fA-F]+",
                  "TYPE": "[^:]+"
              }
          }
      },
      {
          "remove": {
              "field": "message"
          }
      }
    ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
